<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE tiles-definitions PUBLIC       
	"-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"       
	"http://tiles.apache.org/dtds/tiles-config_2_0.dtd">
<tiles-definitions>
    <definition name="baseLayout" template="/tiles/layout.jsp">
        <put-attribute name="header" value="/tiles/header.jsp" />
        <put-attribute name="body" value="" />
        <put-attribute name="footer" value="/tiles/footer.jsp" />
    </definition>
    <definition name="form" extends="baseLayout">
        <put-attribute name="body" value="/form.jsp" />
    </definition>
    <definition name="inicio" extends="baseLayout">
        <put-attribute name="body" value="/inicio.jsp" />
    </definition>
    <definition name="resultado" extends="baseLayout">
        <put-attribute name="body" value="/resultado.jsp" />
    </definition>
    <definition name="error" extends="baseLayout">
        <put-attribute name="body" value="/error.jsp" />
    </definition>
    
</tiles-definitions>